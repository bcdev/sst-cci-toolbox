#!/bin/sh

export PGPORT=${mms.pg.port}
export PGHOST=${mms.pg.host}
export PGDATA=${mms.pg.data}

if [ -n "${LD_LIBRARY_PATH}" ]
then
    export LD_LIBRARY_PATH=${mms_pg_home}/lib:${LD_LIBRARY_PATH}
else
    export LD_LIBRARY_PATH=${mms_pg_home}/lib
fi

export PATH=${mms_home}/bin:${mms_pg_home}/bin:${PATH}

echo "using MMS software in ${mms_home}. Path is"
echo ${PATH}
